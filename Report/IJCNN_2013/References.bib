    month = nov,
    keywords = {Computational Tools, Non-Parametric Methods, Polymorphism},
    title = {A case-control study of non-parametric approaches for detecting SNP-SNP interactions},
    url = {http://jcc2011.utalca.cl/actas/SCCC/jcc2011_submission_111.pdf},
    year = {2011}
}

